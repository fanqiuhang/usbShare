package com.edata.bridge.usbshare;

import lombok.Data;

/**
 * 端口类,基类
 * 这个端口类包含了所有方法的返回数据，所以返回的端口，可能有的属性是没有值的，取自己需要的就可以
 */
@Data
public class UsbsharePortBasic {

    /**
     * 端口 busid
     */
    protected String busid;

    /**
     * usb设备类型
     */
    protected String deviceType;

    /**
     * usb设备协议类型
     */
    protected String deviceProtocolType;

    /**
     * usb设备 PID
     */
    protected String devicePID;

    /**
     * usb 设备VID
     */
    protected String deviceVID;

    /**
     * usb支持的接口数
     */
    protected String supportedNumber;

    /**
     * usb设备名称
     */
    protected String deviceName;





}
